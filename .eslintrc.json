{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "plugins": ["only-warn"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "linebreak-style": "off",
      "no-console": "error",
      "no-alert": "off",
      "max-len":"off"
    }
}
